{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Gatsby: Develop",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/.bin/gatsby",
      "args": [
        "develop"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Gatsby: Build",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/.bin/gatsby",
      "args": [
        "build"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Debug: Tests",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "--inspect-brk",
        "${workspaceRoot}/node_modules/.bin/jest",
        "--runInBand"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
