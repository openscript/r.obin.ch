{
  "extends": "astro/tsconfigs/strictest",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist",
    "vitest.config.ts"
  ],
  "compilerOptions": {
    "moduleResolution": "bundler",
    "exactOptionalPropertyTypes": false,
    "experimentalDecorators": true,
    "useDefineForClassFields": false,
    "types": [
      "unplugin-icons/types/astro"
    ],
  }
}
